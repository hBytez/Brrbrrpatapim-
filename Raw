local function showBadge(text)
    local badge = Instance.new("TextLabel")
    badge.Size = UDim2.new(0, 220, 0, 50)
    badge.Position = UDim2.new(0.5, -110, 0.1, 0)
    badge.BackgroundColor3 = Color3.fromRGB(55, 155
