local function loadInfiniteYield()
    -- Replace the URL below with a valid Infinite Yield URL if needed
    local url = "loadstring(game:HttpGet('https://raw.githubusercontent.com/DarkNetworks/Infinite-Yield/main/latest.lua'))()"
    local success, err = pcall(function()
        loadstring(game:HttpGet(url, true))()
    end)
    if success then
        printDebug("Infinite Yield loaded successfully.")
    else
        printDebug("Failed to load Infinite Yield: ".. tostring(err))
    end
end

